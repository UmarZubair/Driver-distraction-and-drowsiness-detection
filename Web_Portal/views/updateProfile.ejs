    <style>
        .input-icon-wrap {
            border: 0px solid #ddd;
            display: flex;
            flex-direction: row;
        }

        .input-with-icon {
            border: none;
            flex: 1;
            padding: 10px;
        }

        .input-icon {
            padding-left: 10px;
            padding-right: 10px;
            padding-top: 8px;
            padding-bottom: 5px;
            background: rgb(122, 202, 248);
        }

        .cross-icon {
            padding-left: 10px;
            padding-right: 10px;
            padding-top: 8px;
            padding-bottom: 5px;
            background-color: #fc4040;
            color: #fff;
        }

        .input-icon :hover,
        .cross-icon :hover {
            opacity: .5;
        }

        #save,
        #savePic {
            background-color: #aadef3;
            font-size: 17px;
            padding: 10px 20px;
        }

        #save:hover,
        #savePic:hover {
            background-color: rgb(122, 202, 248);
            color: #0f130f;
        }

        .card {
            padding-top: 20px;
            margin: 10px 0 0 0;
            background-color: rgba(214, 224, 226, 0.2);
            border-top-width: 0;
            border-bottom-width: 2px;
            -webkit-border-radius: 3px;
            -moz-border-radius: 3px;
            border-radius: 3px;
            -webkit-box-shadow: none;
            -moz-box-shadow: none;
            box-shadow: none;
            -webkit-box-sizing: border-box;
            -moz-box-sizing: border-box;
            box-sizing: border-box;
        }

        .card.hovercard {
            position: relative;
            padding-top: 0;
            overflow: hidden;
            text-align: center;
            background-color: rgba(214, 224, 226, 0.1);
        }

        .card.hovercard .cardheader {
            background: url("../images/header-bg.png");
            background-size: cover;
            height: 200px;
        }

        .card.hovercard .avatar {
            position: relative;
            top: -85px;
            margin-bottom: -70px;
        }

        .card.hovercard .avatar img {
            width: 200px;
            height: 200px;
            max-width: 200px;
            max-height: 200px;
            -webkit-border-radius: 50%;
            -moz-border-radius: 50%;
            border-radius: 50%;
            border: 5px solid rgb(255, 255, 255);
        }

        .card.hovercard .info {
            padding: 0px 8px 10px;
        }

        .card.hovercard .info .title {
            margin-bottom: 4px;
            font-size: 24px;
            line-height: 1;
            color: #262626;
            vertical-align: middle;
        }

        #main-menu>li.active>a,
        #main-menu>li.active>a:focus,
        #main-menu>li.active>a:hover {
            color: #ffffff;
            cursor: pointer;
            background-color: #fc4040;
            border: 1px solid #fc4040;
            border-bottom-color: transparent
        }

        .widget-tabs-list .nav-tabs>li {
            font-size: 16px;
            margin-right: 30px;
        }

        .widget-tabs-list .nav-tabs>li.active {
            font-weight: bold;
        }

        #del-driver {
            cursor: pointer;
            font-size: 14px;
            background-color: #e64141;
            color: #fff;
            padding: 6px 16px;
            margin-bottom: 2px;
            margin-right: 10px;

        }

        #del-driver:hover {
            background-color: #d83737;

        }

        .form-ic-cmp i {
            font-size: 20px;
            color: #4267b2;
        }

        .form-control {
            font-size: 15px;
        }

        .form-group {
            margin-bottom: 25px;
        }

        .nk-int-mk h2 {
            color: #4267b2;
        }

        #edit-pic {
            cursor: pointer;
            font-size: 17px;
            background-color: #3fbd4f;
            color: #fff;
            padding: 10px 20px;

        }

        #edit-pic:hover {
            background-color: #4cc460;

        }

        /* The Modal (background) */
        .customModal {
            display: none;
            position: fixed;
            z-index: 1;
            padding-top: 200px;
            left: 0;
            top: 0;
            width: 100%;
            height: 100%;
            /* Full height */
            overflow: auto;
            /* Enable scroll if needed */
            background-color: rgb(0, 0, 0);
            /* Fallback color */
            background-color: rgba(0, 0, 0, 0.4);
            /* Black w/ opacity */
            font-family: Helvetica, Arial, sans-serif;
        }

        /* Modal Content */
        .customModal-content {
            font-family: Roboto, Helvetica, Arial, sans-serif;
            position: relative;
            background-color: #fefefe;
            margin: auto;
            padding: 0;
            border: 1px solid #888;
            width: 40%;
            box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2), 0 6px 20px 0 rgba(0, 0, 0, 0.19);
            -webkit-animation-name: animatetop;
            -webkit-animation-duration: 0.4s;
            animation-name: animatetop;
            animation-duration: 0.4s
        }

        @media only screen and (max-width: 600px) {
            .customModal-content {
                width: 90%;
            }
        }

        /* Add Animation */
        @-webkit-keyframes animatetop {
            from {
                top: -300px;
                opacity: 0
            }

            to {
                top: 0;
                opacity: 1
            }
        }

        @keyframes animatetop {
            from {
                top: -300px;
                opacity: 0
            }

            to {
                top: 0;
                opacity: 1
            }
        }

        #confirmRouterDel .customModal-header {
            padding: 10px 16px;
            background-color: #fc4040;
            color: white;
        }

        .customModal-body {
            font-size: 16px;
            padding: 2px 16px;
        }

        .customModal-footer {
            padding: 21px 16px;
            background-color: #F8FCF7;
            text-align: right;
        }

        #confirmRouterDel .modalBtn {
            text-decoration: none;
            background-color: #fc4040;
            color: white;
            padding: 10px 30px;
            border-radius: 4px;
            cursor: pointer;
            border: 0px;
        }

        #confirmRouterDel .modalBtn:hover {
            background-color: #fc4040;
            box-shadow: 2px 5px 5px #EDF2ED;
        }
    </style>
    <script>
        function create(rid) {
            $('#confirmRouterDel').css('display', 'block');
            var elem = document.getElementById("YesBtn");
            var att = document.createAttribute("onclick");
            att.value = "$.ajax({ url: '/crud/delDriver/?id=" + rid + "', method: 'delete',}) .done(function() { console.log('deleted');  window.location.href='/crud/list-of-drivers'; });";
            elem.setAttributeNode(att);
        }
    </script>
</head>

