    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js"></script>
    <script src="http://cdnjs.cloudflare.com/ajax/libs/jquery.form/3.51/jquery.form.min.js"></script>
    <script>
        function editName(id, toShow, toRemove) {
            document.getElementById(id).readOnly = false;
            document.getElementById(id).style.border = '1px solid rgb(122, 202, 248)';
            document.getElementById(toShow).style.display = 'block';
            document.getElementById(toRemove).style.display = 'none';
        }
        function crossName(id, toShow, toRemove, value) {
            document.getElementById(id).readOnly = true;
            document.getElementById(id).style.border = '0px solid rgb(122, 202, 248)';
            document.getElementById(id).style.borderBottom = '1px solid #ccc';
            document.getElementById(id).value = value;
            document.getElementById(toShow).style.display = 'block';
            document.getElementById(toRemove).style.display = 'none';
        }

        function myFunction() {
            var x = document.getElementById("pwd");
            if (x.type === "password") {
                x.type = "text";
            } else {
                x.type = "password";
            }
        }
        function editPic(id) {
            document.getElementById(id).style.display = 'block';
        }
        function readURL(input) {
            if (input.files && input.files[0]) {
                var reader = new FileReader();
                reader.onload = function (e) {
                    $('#profilePic').attr('src', e.target.result);
                }
                reader.readAsDataURL(input.files[0]);
            }
        }
        $(document).ready(function () {
            $("#userPhoto").change(function () {
                readURL(this);
            });
            $('#uploadForm').submit(function () {
                $("#status").empty().text("File is uploading...");
                $(this).ajaxSubmit({

                    error: function (xhr) {
                        status('Error: ' + xhr.status);
                    },

                    success: function (response) {
                        console.log(response)
                        $("#status").empty().text(response);
                    }
                });
                return false;
            });
        });
        $("#edit-pic").click(function () {
            $("#savePic").toggle();
            $("#edit-pic").toggle();
        });
    </script>
